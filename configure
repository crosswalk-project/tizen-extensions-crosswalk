if [ ! `which gyp` ]; then
   echo -e "\nPlease make sure gyp is in your PATH. It is usually found at CHROMIUM_SRC/tools/gyp."
   exit 1
fi

gyp -D build=Debug -D type=desktop --depth=. tizen-wrt.gyp
